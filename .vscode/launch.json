  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug CLI",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/main.go",
      "args": [
        "generate",
        "sdk",
        "-s",
        "../speakeasy-registry/web/packages/docsite/static/openapi.yaml",
        "-o",
        "../speakeasy-client-sdk-python",
        "-l",
        "python"
      ]
    },
    {
      "name": "Debug Docs",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/cmd/docs/main.go"
    }
  ]
}
